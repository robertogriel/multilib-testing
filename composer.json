{
  "name": "robertogriel/multilib-testing",
  "description": "A repository with multiple PHP libraries.",
  "autoload": {
    "psr-4": {
      "Libs\\": "libs/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "./libs/up-stuff",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "./libs/down-things",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "path",
      "url": "./libs/drop-data",
      "options": {
        "symlink": true
      }
    }
  ],
  "require": {
    "robertogriel/down-things": "*",
    "robertogriel/up-stuff": "1.0",
    "robertogriel/drop-data": "*"
  }
}
